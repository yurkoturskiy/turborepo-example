{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "declaration": false,
    "declarationMap": false,
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true
  },
  "include": ["src", "next-env.d.ts"],
  "exclude": ["node_modules"]
}
